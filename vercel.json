{
  "version": 2,
  "name": "bloggers-platform-migrations",
  "buildCommand": "yarn build",
  "env": {
    "NODE_ENV": "development"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*.ts",
          "migrations/**/*.ts",
          "src/env/**/*"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js"
    }
  ]
}